{
  "author": "Markus F. Hay <markus.hay@gmail.com>",
  "name": "react-redux-material",
  "version": "0.1.0",
  "description": "A server agnostic start-up project for front-end apps using React/Redux for developement. Includes Material-UI library for commonly used components.",
  "keywords": [
    "boilerplate",
    "starter project",
    "template",
    "react",
    "redux",
    "material",
    "material-ui"
  ],
  "bugs": "https://github.com/semi-sentient/react-redux-material",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/semi-sentient/react-redux-material"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "start": "cross-env NODE_ENV=dev webpack-dev-server",
    "test:windows": "cross-env NODE_PATH=./src mocha --compilers js:babel-core/register --require ignore-styles --require ./test.config.js --recursive ./src/**/*.spec.js",
    "test:windows-coverage": "cross-env NODE_PATH=./src istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require ignore-styles --require ./test.config.js --recursive ./src/**/*.spec.js",
    "test:linux": "NODE_PATH=./src mocha --compilers js:babel-core/register --require ignore-styles --require --require ./test.config.js './{,!(node_modules)/**/}*.spec.js'",
    "test:linux-coverage": "NODE_PATH=./src istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require ignore-styles --require ./test.config.js './{,!(node_modules)/**/}*.spec.js'"
  },
  "dependencies": {
    "axios": "0.16.x",
    "immutable": "3.8.x",
    "lodash": "4.17.x",
    "material-ui": "0.18.x",
    "moment": "2.18.x",
    "normalize.css": "7.0.x",
    "prop-types": "15.5.x",
    "react": "15.6.x",
    "react-dom": "15.6.x",
    "react-redux": "5.0.x",
    "react-router-dom": "4.1.x",
    "react-router-redux": "5.0.0-alpha.6",
    "react-tap-event-plugin": "2.0.x",
    "redux": "3.7.x",
    "redux-thunk": "2.2.x"
  },
  "devDependencies": {
    "babel-core": "6.24.x",
    "babel-eslint": "7.2.x",
    "babel-loader": "7.1.x",
    "babel-plugin-transform-class-properties": "6.24.x",
    "babel-plugin-transform-object-rest-spread": "6.23.x",
    "babel-polyfill": "6.23.x",
    "babel-preset-latest": "6.22.x",
    "babel-preset-react": "6.23.x",
    "babel-register": "6.24.x",
    "chai": "4.0.x",
    "chai-as-promised": "6.0.x",
    "copy-webpack-plugin": "4.0.x",
    "cross-env": "5.0.x",
    "css-loader": "0.28.x",
    "enzyme": "2.9.x",
    "eslint": "4.3.x",
    "eslint-config-airbnb": "15.1.x",
    "eslint-import-resolver-webpack": "0.8.x",
    "eslint-plugin-import": "2.2.x",
    "eslint-plugin-jsx-a11y": "6.0.x",
    "eslint-plugin-react": "6.10.x",
    "extract-text-webpack-plugin": "2.1.x",
    "html-webpack-plugin": "2.28.x",
    "ignore-styles": "5.0.x",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "10.1.x",
    "mocha": "3.2.x",
    "node-sass": "4.5.x",
    "react-addons-test-utils": "15.6.x",
    "react-test-renderer": "15.6.x",
    "redux-mock-store": "1.2.x",
    "sass-loader": "6.0.x",
    "sinon": "2.3.x",
    "style-loader": "0.18.x",
    "webpack": "3.3.x",
    "webpack-dev-server": "2.4.x"
  }
}
